Update Person 
Set UpdateBy = 'Qais'

Update Person 
Set UpdateBy = 'Not Qais'
Where UpdateBy is not null

Update Person 
Set UpdateBy = 'Qais',UpdateDate = GetDate()
Where UpdateBy is not null

Update Activity 
Set UpdateBy = 'Qais',UpdateDate = GetDate()
Where UpdateBy is  null

